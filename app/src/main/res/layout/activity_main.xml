<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lblTitulo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Servicios Web"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />

    <Button
        android:id="@+id/btnPrimerServicio"
        android:layout_width="119dp"
        android:layout_height="57dp"
        android:text="Lista de Nombres"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.195" />

    <Button
        android:id="@+id/btnSegundoServicio"
        android:layout_width="124dp"
        android:layout_height="52dp"
        android:text="Acerca de mi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPrimerServicio"
        app:layout_constraintVertical_bias="0.087" />

    <Button
        android:id="@+id/btnTercerServicio"
        android:layout_width="137dp"
        android:layout_height="53dp"
        android:text="Respuesta de la suma"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.799"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.199" />

    <Button
        android:id="@+id/btnCuartoServicio"
        android:layout_width="129dp"
        android:layout_height="52dp"
        android:text="Suma"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.776"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnTercerServicio"
        app:layout_constraintVertical_bias="0.087" />

    <Button
        android:id="@+id/btnQuintoServicio"
        android:layout_width="132dp"
        android:layout_height="57dp"
        android:text="Cuadrado"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSegundoServicio"
        app:layout_constraintVertical_bias="0.081" />

    <Button
        android:id="@+id/btnSextoServicio"
        android:layout_width="137dp"
        android:layout_height="55dp"
        android:layout_marginBottom="144dp"
        android:text="Circulo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.799"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCuartoServicio"
        app:layout_constraintVertical_bias="0.128" />

    <Button
        android:id="@+id/btnSeptimoServicio"
        android:layout_width="145dp"
        android:layout_height="53dp"
        android:layout_marginBottom="60dp"
        android:text="Paralelogramo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnQuintoServicio"
        app:layout_constraintVertical_bias="0.13" />

    <Button
        android:id="@+id/btnTrinomio"
        android:layout_width="139dp"
        android:layout_height="55dp"
        android:layout_marginTop="32dp"
        android:text="Trinomio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.805"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSextoServicio" />


</androidx.constraintlayout.widget.ConstraintLayout>